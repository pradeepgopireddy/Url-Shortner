<%if @logs.empty? %>
	<p>No logs found </p>
<% else %>
<table>
	<thead>
		<tr>
			<th> # </th>
			<th> Original_Url </th>
			<th> Short Url </th>
			<th> Clicks </th>
			<th> Browser </th>
			<th> Operating System </th>
			<th> Device </th>
		</tr>
	</thead>
	<tbody>
		<% @logs.each_with_index do |log, index| %>
		<tr>
			<td><%= index + 1 %></td>
			<td><%= log.shortened_url.original_url %></td>
			<td><%= log.shortened_url.short_url %></td>
			<td><%= log.shortened_url.clicks %></td>
			<td><%= log.browser_name %></td>
			<td><%= log.operating_system %></td>
			<td><%= log.device_name %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<% end %>